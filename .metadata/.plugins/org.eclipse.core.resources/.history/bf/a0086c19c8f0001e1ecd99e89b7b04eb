package com.beas.repo;

import org.springframework.data.jpa.repository.JpaRepository;
import org.springframework.data.jpa.repository.Modifying;
import org.springframework.data.jpa.repository.Query;

import com.beas.entity.Address;

import jakarta.transaction.Transactional;

public interface IAddress extends JpaRepository<Address, Integer> {

	@Modifying
	@Transactional
	@Query(value = " insert into address values (city) ", nativeQuery = true)
	public void insertData( String city);

}
